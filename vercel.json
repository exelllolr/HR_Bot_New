{
       "version": 2,
       "framework": null,
       "functions": {
         "api/webhook.py": {
           "handler": "webhook",
           "runtime": "python3.9"
         }
       },
       "routes": [
         {
           "src": "/webhook",
           "dest": "api/webhook.py"
         }
       ],
       "env": [
         "TELEGRAM_TOKEN",
         "DEEPSEEK_API_KEY",
         "DB_NAME",
         "DB_USER",
         "DB_PASSWORD",
         "DB_HOST",
         "DB_PORT"
       ]
     }